<h1>Welcome#index</h1>
<p>Find me in app/views/welcome/index.html.erb</p>
<table>
  <thead>
  <tr>
    <th>User</th>
    <th>Title</th>
    <th>Description</th>
    <th>Open</th>
    <th>Location</th>
    <th>Solved</th>
    <th>Date</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @posts.each do |post| %>
    <tr>
      <td><%= post.user %></td>
      <td><%= post.title %></td>
      <td><%= post.description %></td>
      <td><%= post.open %></td>
      <td><%= post.location %></td>
      <td><%= post.solved %></td>
      <td><%= post.date %></td>
      <td><%= link_to 'Show', post %></td>
      <td><%= link_to 'Edit', edit_post_path(post) %></td>
      <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>
